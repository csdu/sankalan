{
  "version": 2,
  "name": "sankalan",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*\\.(js|json|css|ico|png)$)",
      "dest": "/$1",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" }
    }
  ],
  "alias": "sankalan",
  "builds": [
    { "use": "@now/static-build", "src": "package.json", "config": { "distDir": "_sites" } }
  ]
}
